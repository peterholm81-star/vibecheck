{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/((?!api|assets|favicon.ico|robots.txt|manifest.webmanifest|sw.js|workbox-.*\\.js|icon-.*).*)",
      "destination": "/index.html"
    }
  ]
}

